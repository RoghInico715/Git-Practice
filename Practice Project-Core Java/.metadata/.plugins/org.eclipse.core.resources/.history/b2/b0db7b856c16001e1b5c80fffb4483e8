package PracticeProjectAssistedPractice;

public class accessSpecifiers1 extends accessSpecifiers{

	public static void main(String[] args) {
		
		accessSpecifiers ac = new accessSpecifiers();
		//default
		ac.defaultAccess();
		//public
		ac.publicAccess();
		//protected
		ac.protectedAccess();
		//The private access specifier did not allow us to access its methods outside the class
	}

}
