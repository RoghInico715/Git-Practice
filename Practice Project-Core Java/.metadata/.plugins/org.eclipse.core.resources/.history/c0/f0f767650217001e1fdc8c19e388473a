package collection;

import java.util.HashSet;

public class hashSet {

	public static void main(String[] args) {
		//Create HashSet
		HashSet<String> subject = new HashSet<>();
						
		//Storing elements into the HashSet
		subject.add("English");
		subject.add("Mathematics");
		subject.add("Biology");
		subject.add("Chemistry");
		subject.add("Physics");
						
		//Retrieving items from HashSet
		for (String t : subject) {
			System.out.println(t);
		}
						
		//Methods in HashSet
		//Size of HashSet
		System.out.println("Total elements in the HashSet : " + subject.size());
					
		//Check for a particular element
		System.out.println("Biology is in the list : " + subject.contains("Biology"));
		System.out.println("History is in the list : " + subject.contains("History"));

	}

}
